html,
body {
  margin: 0;
  height: 100%;
  overflow: auto;
}

ul {
  list-style: none;
}

div.row {
  width: 100%;
  height: 100%;
  /* display: flex; */
  border: 1px solid #003458;
}

div.left-side {
  width: 20%;
  height: 100%;
  float: left;
  position: fixed;
  box-sizing: border-box;
  /* background-color: aqua; */
  background-image: url("../image/banner.jpg");
  background-size: cover;
}

div.main {
  width: 80%;
  /* position: relative; */
  float: right;
  padding-left: 5%;
  box-sizing: border-box;
  background-color: blanchedalmond;
}

div.main-top {
  width: 100%;
  height: 80%;
  float: right;
}

div.main-bottom {
  width: 100%;
  height: 20%;
  box-sizing: border-box;
  /* background-color: #003458; */
}

.delBtn {
  width: 80px;
  margin-left: 10%;
  /* position: fixed; */
  left: 80%;
  display: inline-block;
  font-weight: lighter;
  cursor: pointer;
}

div.one {
  height: 50%;
}

/* Style the "Add" button */
.addBtn {
  padding: 10px;
  width: 25%;
  background: #d9d9d9;
  color: #555;
  float: right;
  text-align: center;
  font-size: 16px;
  cursor: pointer;
  transition: 0.3s;
  border-radius: 0;
  height: 40px;
  display: flex;
  align-items: center;
  justify-content: center;
}
